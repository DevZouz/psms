
<div class="col-md-12">
    <div class="row spotify-track-list">
        <div class="col-md-12">
            <img class="track-cover" src={{  images.[0].url }}>
            </br>
            <input type="checkbox" name="track" value={{ track.id }} class="saveToSpotify">
            <span class="{{ id }}_title">{{ title }}</span>  <span class="{{ id }}_artist">({{ artist }})</span>  
        </div>
    </div>
</div>